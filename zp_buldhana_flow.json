{
  "bot_metadata": {
    "bot_name": "Sevadoot - ZP Buldhana Helpline",
    "version": "1.0.0",
    "created_date": "2025-01-15",
    "organization": "Zilla Parishad Buldhana",
    "default_language": "mr",
    "supported_languages": ["mr", "en"],
    "session_config": {
      "timeout_minutes": 60,
      "idle_followup_enabled": true,
      "max_retries": 3,
      "enable_analytics": true
    }
  },

  "global_triggers": {
    "restart": {
      "keywords": ["restart", "पुन्हा सुरू करा", "start", "सुरू", "reset"],
      "action": "go_to_initial",
      "clear_session": true
    },
    "main_menu": {
      "keywords": ["main menu", "मुख्य मेनू", "menu", "मेनू", "home", "होम"],
      "action": "go_to_main_menu",
      "preserve_language": true
    },
    "help": {
      "keywords": ["help", "मदत", "?", "सहाय्य"],
      "action": "show_help",
      "message_key": "help_message"
    }
  },

  "conversation_states": {
    "INITIAL": {
      "state_id": "welcome_language_selection",
      "state_name": "Welcome Screen",
      "state_type": "interactive_button",
      "entry_actions": ["clear_session", "log_session_start"],
      "message_config": {
        "message_key": "welcome_message",
        "media_type": "text",
        "emoji_enabled": true
      },
      "interaction_config": {
        "type": "buttons",
        "max_buttons": 2,
        "timeout_seconds": 300
      },
      "buttons": [
        {
          "id": "btn_select_marathi",
          "button_type": "reply",
          "label_key": "btn_marathi_label",
          "icon": null,
          "next_state": "LANGUAGE_SELECTED_MR",
          "action": "set_language_marathi"
        },
        {
          "id": "btn_select_english",
          "button_type": "reply",
          "label_key": "btn_english_label",
          "icon": null,
          "next_state": "LANGUAGE_SELECTED_EN",
          "action": "set_language_english"
        }
      ],
      "fallback": {
        "invalid_response_message_key": "invalid_language_selection",
        "retry_limit": 3,
        "on_max_retry": "INITIAL"
      }
    },

